<div class="modal fade" id="new-item-lot-modal" tabindex="-1" role="dialog" aria-labelledby="ModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" ><%= "New Item Lot for #{@item.id}" %></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        This is where the form goes
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
        <%= link_to "Logout", logout_path, method: 'delete', class: "btn btn-primary", id: "#{action_name}-logout-confirm-link" %> 
      </div>
    </div>
  </div>
</div>